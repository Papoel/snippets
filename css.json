{
	"Squelette CSS Crée le 06/11/20": {
		"prefix": "!",
		"body": [
		  "/* --------------------------------------------------------------",
		  "                        Partie Générale",
		  " ----------------------------------------------------------------- */",
		  " @import url('https://fonts.googleapis.com/css2?family=Poppins&display=swap');",
		  "",
		  " :root {",
		  "     --color-project:  #069370;",
		  "     --shadow-color-project: rgba(6, 147, 112, 0.5);",
		  " }",
		  "",
		  "body {",
		  "    margin: 0;",
		  "    padding: 0;",
		  "    font-family: 'Poppins', san-serif;",
		  "    font-size: 20px;",
		  "}",
		  "",
		  "/*/  BARRE DE NAVIGATION RESPONSIVE AVEC BOUTON TOGGLE / */",
		  "header {",
		  "    position: relative;",
		  "    max-width: 100%;",
		  "    margin: 20 auto;",
		  "    padding: 10px;",
		  "    background: #fff;",
		  "    border-radius: 4px;",
		  "    box-sizing: border-box;",
		  "    box-shadow: 0 2px 5px var(--shadow-color-project);",
		  "}",
		  ".logo {",
		  "    color: var(--color-project);",
		  "    height: 50px;",
		  "    font-size: 36px;",
		  "    line-height: 60px;",
		  "    padding: 0 20px;",
		  "    text-align: center;",
		  "    box-sizing: border-box;",
		  "    float: left;",
		  "    font-weight: 700;",
		  "    text-decoration: none;",
		  "}",
		  "nav {",
		  "    float: right;",
		  "}",
		  ".clearfix {",
		  "    clear: both;",
		  "}",
		  "nav ul {",
		  "    margin: 0;",
		  "    padding: 0;",
		  "    display: flex;",
		  "}",
		  "nav ul li {",
		  "    list-style: none;",
		  "}",
		  "nav ul li a {",
		  "    display: block;",
		  "    margin: 10px 0;",
		  "    padding: 10px 20px;",
		  "    text-decoration: none;",
		  "    color: #262626;",
		  "}",
		  "nav ul li a.active,",
		  "nav ul li a:hover {",
		  "    background-color: var(--color-project);",
		  "    color: #fff;",
		  "    transition: 0.5s;",
		  "}",
		  "",
		  "/*/ ************** PARTIE DU RESPONSIVE ********************** /*/",
		  "@media (max-width:1200px)",
		  "{",
		  "    header  {",
		  "        margin: 20px;",
		  "    }",
		  "",
		  "@media (max-width:768px)",
		  "{",
		  "    .menu-toggle",
		  "    {",
		  "        display: block;",
		  "        margin: 10px;",
		  "        width: 40px;",
		  "        height: 40px;",
		  "        float: right;",
		  "        cursor: pointer;",
		  "        text-align: center;",
		  "        font-size: 30px;",
		  "        color: var(--color-project);    }",
		  "    .menu-toggle:before",
		  "    {",
		  "        /*- f0c9 => Code font awesome style hamburger -*/",
		  "        content: '\\f0c9';",
		  "        font-family: fontAwesome;",
		  "        line-height: 40px;",
		  "    }",
		  "    .menu-toggle.active:before",
		  "    {",
		  "        /*- f0c9 => Code font awesome style hamburger -*/",
		  "        content: '\\f00d';",
		  "        font-family: fontAwesome;",
		  "        line-height: 40px;",
		  "    }",
		  "    nav",
		  "    {",
		  "        display: none;",
		  "    }",
		  "    }",
		  "    nav.active",
		  "    {",
		  "        display: block;",
		  "        width: 100%;",
		  "    }",
		  "    nav.active ul",
		  "    {",
		  "        display: block;",
		  "    }",
		  "    nav.active ul li a",
		  "    {",
		  "        margin: 0;",
		  "    }",
		  "",
		  "}",
		  "/*/ ************** FOOTER ********************** /*/",
		  ".footer {",
		  "    position: absolute;",
		  "    bottom: 0;",
		  "    width: 100vw;",
		  "}",
		  ".inner-footer {",
		  "    padding: 0;",
		  "    margin: 0;",
		  "    background-color: #262626;",
		  "}",
		  ".social-links {",
		  "    display: flex;",
		  "}",
		  ".social-links ul {",
		  "    padding: 10px;",
		  "    display: flex;",
		  "    width: 300px;",
		  "    height:  30px;",
		  "    margin: auto;",
		  "}",
		  ".social-items {",
		  "    list-style: none;",
		  "}",
		  ".social-items a {",
		  "    padding: 10px 20px;",
		  "    font-size: 2rem;",
		  "    color: var(--color-project);",
		  "    transition: all 0.5s;",
		  "}",
		  ".social-items a:hover {",
		  "    color: #fff;",
		  "    background-color: var(--color-project);",
		  "    border-radius: 50%;",
		  "}",
		  ".quick-links {",
		  "    display: flex;",
		  "    width: 615px;",
		  "    height: 70px;",
		  "    margin: auto;",
		  "}",
		  ".quick-links ul {",
		  "    display: flex;",
		  "}",
		  ".quick-items {",
		  "    list-style: none;",
		  "}",
		  ".quick-items a {",
		  "    text-decoration: none;",
		  "    padding: 0px 15px;",
		  "    font-size: 1.1rem;",
		  "    color: #fff;",
		  "    transition: all 0.5s;",
		  "}",
		  ".quick-items a:hover {",
		  "    color: var(--color-project);",
		  "}",
		  "",
		  ".outer-footer {",
		  "    padding: 10px;",
		  "    text-align: center;",
		  "    color: #fff;",
		  "    font-size: 18px;",
		  "    background-color:#3f3f3f;",
		  "}",
		  "",
		  "/* / PARTIE RESPONSIVE DU FOOTER / */",
		  "@media (max-width:600px)",
		  "{",
		  "    .quick-items a {",
		  "        font-size: .8rem;",
		  "        padding-left: 0;",
		  "    }",
		  "    .outer-footer {",
		  "        font-size: .9rem;",
		  "    }",
		  "}",
		  ""
		],
		"description": "Squelette CSS Crée le 06/11/20"
	  }
	}